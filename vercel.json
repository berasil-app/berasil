{
    "version": 2,
    "builds": [
      {
        "src": "./index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/"
      }
    ],
    "functions": {
      "postBuild": {
        "runtime": "@vercel/node",
        "handler": "npx prisma generate"
      }
    }
  }
  